@model IEnumerable<string>
@{
    Layout = null;
}

@Html.ActionLink("Home", "List", "Product")

@foreach (var link in Model)
{
    @Html.RouteLink(link, new{
            controller="Product",
            action="List",
            category=link,
            page=1
        },
        //@class -- is not Razor descriptor!
        //@class-- c# collision resolver
        new{ @class=(link==ViewBag.CurrentCat) ? "selected":null}              
    );
}

